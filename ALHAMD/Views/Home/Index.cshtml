    .services-section {
        padding: 60px 0;
        background-color: #ffffff;
    }

        .services-section h2 {
            font-size: 36px;
            font-weight: bold;
            margin-bottom: 40px;
            text-align: center;
            color: #003049;
        }

    .service-card {
        position: relative;
        overflow: hidden;
        border-radius: 10px;
        transition: transform 0.3s ease;
        box-shadow: 0px 4px 10px rgba(0, 0, 0, 0.1);
        margin-bottom: 30px;
    }

        .service-card img {
            width: 100%;
            height: auto;
            display: block;
        }

        .service-card:hover {
            transform: scale(1.05);
        }

    .service-overlay {
        position: absolute;
        top: 0;
        left: 0;
        width: 100%;
        height: 100%;
        background: rgba(0, 0, 0, 0.5);
        color: white;
        padding: 20px;
        display: flex;
        flex-direction: column;
        justify-content: flex-start;
        opacity: 1;
        transition: opacity 0.3s ease;
    }

    .service-icon {
        background-color: #c9302c;
        padding: 10px;
        border-radius: 5px;
        display: inline-block;
        margin-bottom: 15px;
    }

        .service-icon img {
            width: 30px;
            height: 30px;
        }

    .service-title {
        font-size: 20px;
        font-weight: bold;
        margin-bottom: 10px;
        text-transform: uppercase;
        color: #ffffff;
    }

    .service-text {
        font-size: 14px;
        line-height: 1.5;
        margin-bottom: 15px;
    }

    .service-arrow {
        font-size: 20px;
        align-self: flex-end;
    }

    /* Specific adjustments */
    .service-card:nth-child(2) .service-overlay,
    .service-card:nth-child(3) .service-overlay,
    .service-card:nth-child(4) .service-overlay,
    .service-card:nth-child(5) .service-overlay,
